<?xml version="1.0"?>
<configuration>
  <connectionStrings />
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>
  
   <appSettings>
    <add key="mcstring" value="Provider=Microsoft.ACE.OLEDb.12.0;Data Source="/>
    <add key="dbPath" value="~/db/Appstore.accdb"/>
   </appSettings >
  <system.webServer>
    <modules>
      <!-- Uploader httpModule -->
      <add name="CuteWebUI.UploadModule" type="CuteWebUI.UploadModule,CuteWebUI.AjaxUploader"/>
    </modules>
  </system.webServer>
</configuration>
